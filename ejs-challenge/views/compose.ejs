<%- include("partials/header"); -%>

<h1>Compose</h1>

<form action="/compose" method="post">
  <div class="form-group">
    <label for="pubTitle">Title</label>
    <input type="text" class="form-control" name="publishTitle" id="pubTitle" />
    <label for="pubText">Post</label>
    <textarea
      class="form-control"
      rows="5"
      cols="30"
      name="publishText"
      id="pubText"
    ></textarea>
  </div>
  <button type="submit" class="btn btn-primary">Publish</button>
</form>
<%- include("partials/footer"); -%>
